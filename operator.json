{
  "name": "Levene Test",
  "description": "Performs Levene test to assess homogeneity of variance across groups.",
  "tags": [
    "statistical testing",
    "linear model"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/levene_test_operator"
  ],
  "container": "tercen/levene_test_operator:0.1.1",
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Row",
            "type": "",
            "description": "Optional row factor.",
            "ontologyMapping": "variable",
            "crosstabMapping": "row",
            "cardinality": "0..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Column",
            "type": "",
            "description": "Optional column factor.",
            "ontologyMapping": "observation",
            "crosstabMapping": "column",
            "cardinality": "0..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Measurement",
                "type": "numeric",
                "description": "Measurement values",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              },
              {
                "kind": "MetaFactor",
                "name": "Groups",
                "type": "",
                "description": "Represents the groups to compare",
                "ontologyMapping": "group",
                "crosstabMapping": "color",
                "cardinality": "1..n",
                "factors": []
              }
            ]
          }
        ]
      }
    ],
    "outputSpecsV2": [
      {
        "kind": "OperatorJoinSpec",
        "joinOperators": [
          {
            "kind": "JoinOperator",
            "joinType": "left",
            "leftPair": {
              "kind": "ColumnPair",
              "lColumns": [
                "Row",
                "Column"
              ],
              "rColumns": [
                "Row",
                "Column"
              ]
            },
            "rightRelation": {
              "kind": "TableRelation",
              "attributes": [
                {
                  "kind": "Attribute",
                  "name": "df1",
                  "type": "double"
                },
                {
                  "kind": "Attribute",
                  "name": "df2",
                  "type": "double"
                },
                {
                  "kind": "Attribute",
                  "name": "statistic",
                  "type": "double"
                },
                {
                  "kind": "Attribute",
                  "name": "p",
                  "type": "double"
                }
              ]
            }
          }
        ]
      }
    ]
  }
}